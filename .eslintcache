dishId, values.rating, values.name, values.comment);    }\n\n    render() {\n        return (\n            <div>\n                <div>\n                    <Button outline onClick={this.toggleModal}><i className=\"fa fa-pencil\"></i> Submit Comment</Button>\n                </div>\n                <Modal isOpen={this.state.isModalOpen} toggle={this.toggleModal}>\n                    <ModalHeader toggle={this.toggleModal}>Submit Comment</ModalHeader>\n                    <ModalBody>\n                        <LocalForm onSubmit={(values) => this.handleSubmit(values)}>\n                            <FormGroup>\n                                <Label htmlFor=\"rating\">Rating</Label>\n                                <Control.select model=\".rating\" name=\"rating\" className=\"form-control\">\n                                    <option>1</option>\n                                    <option>2</option>\n                                    <option>3</option>\n                                    <option>4</option>\n                                    <option>5</option>\n                                </Control.select>\n                            </FormGroup>\n                            <FormGroup>\n                                <Label htmlFor=\"name\">Your Name</Label>\n                                <Control.text model=\".name\" id=\"name\" className=\"form-control\" placeholder=\"Your Name\"\n                                              validators={{\n                                                  minLength: minLength(3), maxLength: maxLength(15)\n                                              }}\n                                />\n                                <Errors model=\".name\"\n                                        className=\"text-danger\"\n                                        show=\"touched\"\n                                        messages={{\n                                            minLength: \"Must be greater than 2 characters\",\n                                            maxLength: \"Must be 15 characters or less\"\n                                        }}\n                                />\n                            </FormGroup>\n                            <FormGroup>\n                                <Label htmlFor=\"comment\">Comment</Label>\n                                <Control.textarea model=\".comment\" rows={6} className=\"form-control\" />\n                            </FormGroup>\n                            <Button type=\"submit\" value=\"submit\" color=\"primary\">Login</Button>\n                        </LocalForm>\n                    </ModalBody>\n                </Modal>\n            </div>\n        );\n    }\n}\n\n\n    function RenderDish({dish}) {\n        return (\n            <FadeTransform\n                in\n                transformProps={{\n                    exitTransform: 'scale(0.5) translateY(-50%)'\n                }}>\n                <Card>\n                    <CardImg top src={baseUrl + dish.image} alt={dish.name} />\n                    <CardBody>\n                        <CardTitle>{dish.name}</CardTitle>\n                        <CardText>{dish.description}</CardText>\n                    </CardBody>\n                </Card>\n            </FadeTransform>\n        );\n    }\n\n    function RenderComments({ comments, postComment, dishId }) {\n        if(comments != null) {\n            const comm = comments.map((comment) => {\n                return (\n                    <Fade in>\n                        <li key={comment.id}>\n                            <p className=\"mb-0\">{comment.comment}</p>\n                            <p>-- {comment.author} at {new Intl.DateTimeFormat('en-US', { year: 'numeric', month: 'short', day: '2-digit'}).format(new Date(Date.parse(comment.date)))}</p>\n                        </li>\n                    </Fade>\n                );\n            });\n\n            return (\n                <div>\n                    <h4>Comments</h4>\n                    <ul className=\"list-unstyled\">\n                        <Stagger in>\n                            {comm}\n                        </Stagger>\n                    </ul>\n                    <CommentForm postComment={postComment} dishId={dishId} />\n                </div>\n            );\n        } else {\n            return (\n                <div></div>\n            );\n        }\n    }\n\n    const DishDetail = (props) => {\n        if (props.isLoading) {\n            return(\n                <div className=\"container\">\n                    <div className=\"row\">\n                        <Loading />\n                    </div>\n                </div>\n            );\n        }\n        else if (props.errMess) {\n            return(\n                <div className=\"container\">\n                    <div className=\"row\">\n                        <h4>{props.errMess}</h4>\n                    </div>\n                </div>\n            );\n        }\n        else if (props.dish != null) {\n            return (\n                <div className=\"container\">\n                    <div className=\"row\">\n                        <Breadcrumb>\n                            <BreadcrumbItem><Link to=\"/menu\">Menu</Link></BreadcrumbItem>\n                            <BreadcrumbItem active>{props.dish.name}</BreadcrumbItem>\n                        </Breadcrumb>\n                        <div className=\"col-12\">\n                            <h3>{props.dish.name}</h3>\n                            <hr />\n                        </div>                \n                    </div>\n                    <div className=\"row\">\n                        <div className=\"col-12 col-md-5 m-1\">\n                            <RenderDish dish={props.dish} />\n                        </div>\n                        <div className=\"col-12 col-md-5 m-1\">\n                            <RenderComments comments={props.comments} postComment={props.postComment} dishId={props.dish.id} />\n                        </div>\n                    </div>\n                </div>\n            );\n        } else {\n            return (<div></div>);\n        }\n    }\n\n\nexport default DishDetail;","/home/aqeelnasrullah/Desktop/Aqeel/Coursera/Front End Developement with React/confusion/src/shared/promotions.js",[],["126","127"],"/home/aqeelnasrullah/Desktop/Aqeel/Coursera/Front End Developement with React/confusion/src/shared/leaders.js",[],["128","129"],"/home/aqeelnasrullah/Desktop/Aqeel/Coursera/Front End Developement with React/confusion/src/reportWebVitals.js",[],["130","131"],"/home/aqeelnasrullah/Desktop/Aqeel/Coursera/Front End Developement with React/confusion/src/components/MenuComponent.js",[],"/home/aqeelnasrullah/Desktop/Aqeel/Coursera/Front End Developement with React/confusion/src/redux/configureStore.js",[],"/home/aqeelnasrullah/Desktop/Aqeel/Coursera/Front End Developement with React/confusion/src/redux/dishes.js",[],["132","133"],"/home/aqeelnasrullah/Desktop/Aqeel/Coursera/Front End Developement with React/confusion/src/shared/dishes.js",[],"/home/aqeelnasrullah/Desktop/Aqeel/Coursera/Front End Developement with React/confusion/src/redux/comments.js",[],"/home/aqeelnasrullah/Desktop/Aqeel/Coursera/Front End Developement with React/confusion/src/components/AboutComponent.js",[],"/home/aqeelnasrullah/Desktop/Aqeel/Coursera/Front End Developement with React/confusion/src/redux/actionCreators.js",[],"/home/aqeelnasrullah/Desktop/Aqeel/Coursera/Front End Developement with React/confusion/src/App.js",[],"/home/aqeelnasrullah/Desktop/Aqeel/Coursera/Front End Developement with React/confusion/src/components/FooterComponent.js",[],"/home/aqeelnasrullah/Desktop/Aqeel/Coursera/Front End Developement with React/confusion/src/redux/leaders.js",[],"/home/aqeelnasrullah/Desktop/Aqeel/Coursera/Front End Developement with React/confusion/src/redux/promotions.js",[],"/home/aqeelnasrullah/Desktop/Aqeel/Coursera/Front End Deve